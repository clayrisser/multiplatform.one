{
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true,
        "image": "latest"
      },
      "env": {
        "TAMAGUI_TARGET": "native"
      }
    },
    "production": {
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "TAMAGUI_TARGET": "native"
      }
    },
    "simulator": {
      "ios": {
        "simulator": true,
        "image": "latest"
      },
      "android": {
        "image": "latest"
      },
      "env": {
        "STAGE": "staging",
        "TAMAGUI_TARGET": "native"
      },
      "node": "16.16.0",
      "yarn": "2.4.3",
      "cache": {
        "key": "pt-2"
      }
    }
  }
}
