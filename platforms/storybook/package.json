    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-react-native-web": "0.0.21",
    "@storybook/addon-storyshots": "^7.6.7",
    "@storybook/addon-storysource": "^7.6.7",
    "@storybook/nextjs": "^7.6.7",
    "@storybook/react": "^7.6.7",
    "addon-screen-reader": "^1.8.6",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "expo": "^49.0.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "~0.73.1",
    "react-native-web": "~0.19.10",
    "storybook-addon-breakpoints": "^1.0.0",
    "storybook-addon-designs": "^6.3.1",
    "storybook-addon-paddings": "^5.0.0",
    "storybook-color-picker": "^4.0.0",
    "storybook-dark-mode": "^3.0.3",
