{
  "name": "@platform/storybook",
  "private": true,
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@etchteam/storybook-addon-status": "^4.2.4",
    "@storybook/addon-a11y": "^8.0.9",
    "@storybook/addon-actions": "^8.0.9",
    "@storybook/addon-essentials": "^8.0.9",
    "@storybook/addon-links": "^8.0.9",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-react-native-web": "0.0.23",
    "@storybook/addon-storyshots": "^7.6.17",
    "@storybook/addon-storysource": "^8.0.9",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/nextjs": "^8.0.9",
    "@storybook/react": "^8.0.9",
    "addon-screen-reader": "^1.8.6",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "expo": "^50.0.17",
    "react": "18.2.0",
    "react-dom": "18.3.0",
    "react-native": "0.73.6",
    "react-native-web": "~0.19.11",
    "storybook-addon-paddings": "^5.0.1",
    "storybook-color-picker": "^4.0.0",
    "storybook-dark-mode": "^4.0.1",
    "stream-browserify": "^3.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@loki/target-chrome-core": "0.35.0",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
    "@tamagui/babel-plugin": "^1.95.1",
    "@types/babel__preset-env": "^7.9.6",
    "@types/node": "~20.12.7",
    "@types/react": "^18.3.1",
    "loki": "^0.35.0",
    "storybook": "^8.0.9",
    "typescript": "5.3.3"
  },
  "loki": {
    "configurations": {
      "chrome.iphone7": {
        "target": "chrome.app",
        "preset": "iPhone 7"
      }
    },
    "diffingEngine": {
      "gm": {},
      "looks-same": {},
      "pixelmatch": {}
    },
    "verboseRenderer": true
  },
  "jest": {
    "testRegex": "((/(tests|__tests__)((/|/[^_.\n][^/\n]*(/[^_.\n][^/\n]+)*/)[^_.\n][^/\n]*))|((/|/.*/)(([^_.\n][^/\n]*.(spec|test))|(spec|test)))).[jt]sx?$",
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/storybook-static/"
    ]
  },
  "jestSonar": {
    "reportPath": "node_modules",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
