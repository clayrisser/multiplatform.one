{
  "name": "@platform/storybook",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@etchteam/storybook-addon-status": "^4.2.2",
    "@storybook/addon-a11y": "^6.5.16",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-ie11": "^0.0.6",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-react-native-web": "0.0.7",
    "@storybook/addon-storyshots": "^6.5.16",
    "@storybook/addon-storysource": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "addon-screen-reader": "^1.8.6",
    "app": "*",
    "expo": "^47.0.13",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-web": "~0.18.11",
    "storybook-addon-breakpoints": "^1.0.0",
    "storybook-addon-designs": "^6.3.1",
    "storybook-addon-grid": "^0.3.1",
    "storybook-addon-next": "^1.7.1",
    "storybook-addon-paddings": "^4.3.0",
    "storybook-addon-themes": "^6.1.0",
    "storybook-color-picker": "^3.1.0",
    "storybook-dark-mode": "^2.0.5",
    "storybook-mobile": "^1.0.0"
  },
  "devDependencies": {
    "@loki/target-chrome-core": "0.31.0",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.27",
    "loki": "^0.31.0",
    "typescript": "^4.9.5"
  },
  "loki": {
    "configurations": {
      "chrome.iphone7": {
        "target": "chrome.app",
        "preset": "iPhone 7"
      }
    },
    "diffingEngine": {
      "gm": {},
      "looks-same": {},
      "pixelmatch": {}
    },
    "verboseRenderer": true
  },
  "jest": {
    "testRegex": "((/(tests|__tests__)((/|/[^_.\n][^/\n]*(/[^_.\n][^/\n]+)*/)[^_.\n][^/\n]*))|((/|/.*/)(([^_.\n][^/\n]*.(spec|test))|(spec|test)))).[jt]sx?$",
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/storybook-static/"
    ]
  },
  "jestSonar": {
    "reportPath": ".mkpm/.tmp/reports",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
