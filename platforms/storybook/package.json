  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@loki/target-chrome-core": "0.31.2",
    "@tamagui/babel-plugin": "^1.28.2",
    "@types/babel__preset-env": "^7.9.2",
    "@types/node": "~18.16.13",
    "@types/react": "^18.2.6",
    "loki": "^0.31.2",
    "storybook": "^7.0.12",
    "typescript": "^4.9.4"
  },
  "loki": {
    "configurations": {
      "chrome.iphone7": {
        "target": "chrome.app",
        "preset": "iPhone 7"
      }
    },
    "diffingEngine": {
      "gm": {},
      "looks-same": {},
      "pixelmatch": {}
    },
    "verboseRenderer": true
  },
  "jest": {
    "testRegex": "((/(tests|__tests__)((/|/[^_.\n][^/\n]*(/[^_.\n][^/\n]+)*/)[^_.\n][^/\n]*))|((/|/.*/)(([^_.\n][^/\n]*.(spec|test))|(spec|test)))).[jt]sx?$",
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/storybook-static/"
    ]
  },
  "jestSonar": {
    "reportPath": ".mkpm/.tmp/reports",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
