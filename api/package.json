    "@graphql-mesh/cli": "^0.96.12",
    "@graphql-mesh/graphql": "^0.102.16",
    "@graphql-mesh/plugin-operation-field-permissions": "^0.102.13",
    "@graphql-mesh/plugin-rate-limit": "^0.102.13",
    "@graphql-mesh/plugin-response-cache": "^0.102.13",
    "@graphql-mesh/transform-filter-schema": "^0.102.13",
    "@graphql-mesh/transform-prefix": "^0.102.13",
    "@multiplatform.one/keycloak-typegraphql": "workspace:1.1.10",
    "@multiplatform.one/prisma-scripts": "workspace:1.1.8",
