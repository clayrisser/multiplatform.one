# app
DEBUG=1
PRIVATE_PORT=5001
PUBLIC_PORT=5000

# prisma
PRISMA_DATABASE_ENGINE=postgres
PRISMA_MIGRATE=1
PRISMA_SEED=1
PRISMA_STUDIO=1
PRISMA_STUDIO_PORT=5555

# security
CORS=1
CORS_ORIGIN="*"

# postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DATABASE=postgres
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# redis
REDIS=1
REDIS_DATABASE=0
REDIS_HOST=localhost
REDIS_PASSWORD=
REDIS_PORT=6379

# keycloak
KEYCLOAK_ADMIN_PASSWORD=P@ssw0rd
KEYCLOAK_ADMIN_USERNAME=admin
KEYCLOAK_BASE_URL=http://localhost:8080
KEYCLOAK_CLIENT_ID=api
KEYCLOAK_CLIENT_SECRET=P@ssw0rd
KEYCLOAK_DEBUG=0
KEYCLOAK_REALM=main

# sqlite
SQLITE_URL=file:./sqlite.db

# swagger
SWAGGER=0
SWAGGER_BASE_PATH=/docs

# sofa
SOFA_BASE_PATH=/api

# graphql
GRAPHQL_PLAYGROUND=0
ENABLE_CACHING=1
DEFAULT_MAX_AGE=60

# telemetry
LOG_AXIOS_DATA=0
LOG_AXIOS_HEADERS=0
LOG_FILE_NAME=docker/data/logs/app.log
LOG_LEVELS=error,warn,log
LOG_PRETTY=0
METRICS_PORT=3100
OTEL_EXPORTER_JAEGER_ENDPOINT=http://localhost:14268/api/traces
OTEL_SERVICE_NAME=app
