{
  "name": "@multiplatform.one/ui",
  "version": "0.0.41",
  "sideEffects": [
    "*.css"
  ],
  "description": "multiplatform.one ui components",
  "main": "dist/cjs",
  "module": "dist/esm",
  "module:jsx": "dist/jsx",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./types/index.d.ts"
    },
    "./src/tamagui.config": {
      "import": "./dist/esm/tamagui.config.mjs",
      "require": "./dist/cjs/tamagui.config.js",
      "types": "./types/tamagui.config.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/risserlabs/multiplatform.one/keycloak"
  },
  "dependencies": {
    "@tamagui/animations-react-native": "^1.16.3",
    "@tamagui/font-inter": "^1.16.3",
    "@tamagui/react-native-media-driver": "^1.16.3",
    "@tamagui/shorthands": "^1.16.3",
    "@tamagui/theme-base": "^1.16.3",
    "expo-asset": "^8.9.1",
    "fast-xml-parser": "^4.2.2",
    "immer": "^10.0.1",
    "react-hook-form": "^7.43.9",
    "react-native-svg": "13.9.0",
    "tamagui": "^1.16.3",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@tamagui/build": "^1.16.3"
  },
  "jest": {
    "testRegex": "((/(tests|__tests__)((/|/[^_.\n][^/\n]*(/[^_.\n][^/\n]+)*/)[^_.\n][^/\n]*))|((/|/.*/)(([^_.\n][^/\n]*.(spec|test))|(spec|test)))).[jt]sx?$",
    "verbose": true,
    "testPathIgnorePatterns": []
  },
  "jestSonar": {
    "reportPath": ".mkpm/.tmp/reports",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
