{
  "name": "@multiplatform.one/prisma-scripts",
  "version": "1.0.1",
  "description": "scripts for managing prisma orm",
  "sideEffects": false,
  "keywords": [
    "cli-cursor",
    "database",
    "db",
    "orm",
    "prisma",
    "prisma-scripts",
    "scripts"
  ],
  "homepage": "https://multiplatform.one",
  "bugs": {
    "url": "https://gitlab.com/bitspur/multiplatform.one/multiplatform.one/issues",
    "email": "support@risserlabs.com"
  },
  "license": "Apache-2.0",
  "author": "BitSpur <support@risserlabs.com> (https://risserlabs.com)",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "files": [
    "bin",
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/bitspur/multiplatform.one/multiplatform.one"
  },
  "bin": {
    "generate-prisma": "bin/generate.js",
    "wait-for-postgres": "bin/waitForPostgres.js"
  },
  "dependencies": {
    "core-js": "^3.37.0",
    "dotenv": "^16.4.5",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "ora": "^8.0.1"
  },
  "peerDependencies": {
    "prisma": "~5.8.1"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.10",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "cspell": "^8.7.0",
    "eslint": "^9.1.1",
    "jest": "^29.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "majestic": "^1.8.1",
    "prettier": "^3.2.5",
    "prisma": "~5.13.0",
    "tsup": "^8.0.2",
    "typescript": "~5.4.5"
  },
  "transpileModules": [
    "#ansi-styles",
    "#supports-color",
    "chalk",
    "cli-cursor",
    "execa",
    "is-interactive",
    "is-unicode-supported",
    "log-symbols",
    "npm-run-path",
    "ora",
    "restore-cursor",
    "stdin-discarder",
    "strip-final-newline"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "eslintIgnore": [
    "lib"
  ],
  "jest": {
    "testRegex": "((/(tests|__tests__)((/|/[^_.\n][^/\n]*(/[^_.\n][^/\n]+)*/)[^_.\n][^/\n]*))|((/|/.*/)(([^_.\n][^/\n]*.(spec|test))|(spec|test)))).[jt]sx?$",
    "testEnvironment": "node",
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/lib/",
      "<rootDir>/dist/"
    ]
  },
  "jestSonar": {
    "reportPath": "node_modules/.tmp/reports",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
