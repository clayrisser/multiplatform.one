{
  "name": "@multiplatform.one/components",
  "version": "2.0.0",
  "sideEffects": [
    "*.css"
  ],
  "description": "multiplatform.one ui components",
  "main": "dist/cjs",
  "module": "dist/esm",
  "module:jsx": "dist/jsx",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./types/index.d.ts"
    },
    "./css/code-highlight.css": {
      "import": "./css/code-highlight.css",
      "require": "./css/code-highlight.css"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rm -rf dist types 2>/dev/null && tamagui-build"
  },
  "files": [
    "css",
    "dist",
    "src",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/bitspur/multiplatform.one/multiplatform.one.git"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@mdx-js/mdx": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@multiplatform.one/locales": "workspace:*",
    "@tamagui/font-inter": "^1.99.1",
    "@tamagui/logo": "^1.99.1",
    "@tamagui/react-native-media-driver": "^1.99.1",
    "@tamagui/shorthands": "^1.99.1",
    "@tamagui/themes": "^1.99.1",
    "burnt": "^0.12.2",
    "expo-asset": "^10.0.6",
    "expo-image": "^1.12.9",
    "fast-xml-parser": "^4.4.0",
    "gray-matter": "^4.0.3",
    "hast-util-to-html": "8.0.4",
    "immer": "^10.1.1",
    "multiplatform.one": "workspace:*",
    "parse-numeric-range": "^1.3.0",
    "react-hook-form": "^7.51.5",
    "react-native-svg": "15.2.0",
    "refractor": "^4.8.1",
    "rehype-autolink-headings": "~6.1.1",
    "rehype-parse": "~8.0.5",
    "rehype-slug": "~5.1.0",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx-frontmatter": "^4.0.0",
    "tamagui": "^1.99.1",
    "unist-util-visit": "^5.0.0"
  },
  "peerDependencies": {
    "moti": "^0.25.3",
    "react-native-reanimated": "2.14.4",
    "solito": "^3.2.6"
  },
  "devDependencies": {
    "@tamagui/build": "^1.99.1",
    "@types/eslint": "^8.56.10",
    "moti": "^0.29.0",
    "react-native-reanimated": "3.11.0",
    "solito": "^4.2.2",
    "tsup": "^8.0.2"
  },
  "eslintIgnore": [
    "dist",
    "types"
  ],
  "jest": {
    "testRegex": "((/(tests|__tests__)((/|/[^_.\n][^/\n]*(/[^_.\n][^/\n]+)*/)[^_.\n][^/\n]*))|((/|/.*/)(([^_.\n][^/\n]*.(spec|test))|(spec|test)))).[jt]sx?$",
    "verbose": true,
    "testPathIgnorePatterns": []
  },
  "jestSonar": {
    "reportPath": "node_modules",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
