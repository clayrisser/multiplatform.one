{
  "name": "@multiplatform.one/utils",
  "version": "0.1.8",
  "description": "utilities for multiplatform.one",
  "sideEffects": false,
  "keywords": [
    "react",
    "react-native",
    "electron"
  ],
  "homepage": "https://multiplatform.one",
  "bugs": {
    "url": "https://gitlab.com/bitspur/multiplatform.one/multiplatform.one/issues",
    "email": "support@risserlabs.com"
  },
  "license": "Apache-2.0",
  "author": "BitSpur <support@risserlabs.com> (https://risserlabs.com)",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    },
    "./dev": {
      "types": "./lib/dev.d.ts",
      "import": "./lib/dev.js",
      "require": "./lib/dev.js"
    },
    "./meshConfig": {
      "types": "./lib/meshConfig.d.ts",
      "import": "./lib/meshConfig.js",
      "require": "./lib/meshConfig.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup"
  },
  "files": [
    "bin",
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/bitspur/multiplatform.one/multiplatform.one"
  },
  "devDependencies": {
    "@types/node": "~20.12.13",
    "@types/pg": "^8.10.9",
    "tsup": "^8.3.5",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "pg": "^8.13.1",
    "yaml": "^2.7.0"
  }
}
