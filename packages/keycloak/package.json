    "url": "https://gitlab.com/risserlabs/multiplatform.one/keycloak"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-navigation/native": "^6.0.14",
    "expo-keycloak-auth": "1.0.14",
