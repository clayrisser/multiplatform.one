{
  "name": "@multiplatform.one/keycloak",
  "version": "1.0.3",
  "description": "keycloak client for multiplatform.one ecosystem",
  "main": "dist/cjs",
  "module": "dist/esm",
  "module:jsx": "dist/jsx",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./types/index.d.ts"
    },
    "./routes": {
      "import": "./dist/esm/routes.js",
      "require": "./dist/cjs/routes.js",
      "types": "./dist/esm/routes.d.ts"
    }
  },
  "author": "BitSpur",
  "sideEffects": false,
  "keywords": [
    "auth",
    "keycloak",
    "multiplatform",
    "multiplatform.one",
    "react"
  ],
  "homepage": "https://multiplatform.one",
  "license": "Apache-2.0",
  "files": [
    "lib",
    "routes",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/risserlabs/multiplatform.one/keycloak"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": "1.21.0",
    "multiplatform.one": "^1.0.3",
    "next": "^13.0.5",
    "react": "18.2.0",
    "react-native": "0.73.6"
  },
  "devDependencies": {
    "@react-native-async-storage/async-storage": "1.21.0",
    "@tamagui/build": "^1.95.1",
    "@types/eslint": "^8.56.10",
    "@types/jest": "^29.5.12",
    "@types/lodash.camelcase": "^4.3.9",
    "@types/lodash.startcase": "^4.4.9",
    "@types/react": "^18.3.1",
    "@types/react-native": "^0.73.0",
    "cspell": "^8.7.0",
    "eslint": "~8.56.0",
    "jest": "^29.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "multiplatform.one": "^1.0.3",
    "next": "^14.2.3",
    "prettier": "^3.2.5",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "eslintIgnore": [
    "lib"
  ],
  "jest": {
    "testRegex": "((/(tests|__tests__)((/|/[^_.\n][^/\n]*(/[^_.\n][^/\n]+)*/)[^_.\n][^/\n]*))|((/|/.*/)(([^_.\n][^/\n]*.(spec|test))|(spec|test)))).[jt]sx?$",
    "testEnvironment": "node",
    "preset": "ts-jest",
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/lib/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/_setup.ts"
    ]
  },
  "jestSonar": {
    "reportPath": "node_modules",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "dependencies": {
    "@apollo/client": "^3.10.1",
    "expo-auth-session": "^5.5.1",
    "expo-crypto": "^12.8.1",
    "keycloak-js": "^24.0.3",
    "next-auth": "^4.24.7"
  }
}
