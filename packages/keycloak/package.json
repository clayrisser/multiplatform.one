{
  "name": "@multiplatform.one/keycloak",
  "version": "0.7.15",
  "description": "keycloak client for multiplatform.one ecosystem",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./route": {
      "import": "./lib/route.mjs",
      "require": "./lib/route.js",
      "types": "./lib/route.d.ts"
    }
  },
  "author": "BitSpur",
  "sideEffects": false,
  "keywords": [
    "auth",
    "keycloak",
    "multiplatform",
    "multiplatform.one",
    "react"
  ],
  "homepage": "https://multiplatform.one",
  "license": "Apache-2.0",
  "files": [
    "dist",
    "index.d.ts",
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/risserlabs/multiplatform.one/keycloak"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": "1.19.3",
    "expo-keycloak-auth": "1.0.14",
    "multiplatform.one": "^0.7.16",
    "next": "^13.0.5",
    "react": "^18.2.0",
    "react-native": "~0.73.1"
  },
  "devDependencies": {
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-navigation/native": "^6.1.9",
    "@types/eslint": "^8.56.1",
    "@types/jest": "^29.5.11",
    "@types/lodash.camelcase": "^4.3.9",
    "@types/lodash.startcase": "^4.4.9",
    "@types/react": "^18.2.47",
    "@types/react-native": "^0.73.0",
    "cspell": "^8.3.2",
    "eslint": "^8.56.0",
    "expo-keycloak-auth": "1.0.14",
    "jest": "^29.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "multiplatform.one": "^0.7.16",
    "next": "^13.5.6",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-native": "~0.73.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "eslintIgnore": [
    "lib"
  ],
  "jest": {
    "testRegex": "((/(tests|__tests__)((/|/[^_.\n][^/\n]*(/[^_.\n][^/\n]+)*/)[^_.\n][^/\n]*))|((/|/.*/)(([^_.\n][^/\n]*.(spec|test))|(spec|test)))).[jt]sx?$",
    "testEnvironment": "node",
    "preset": "ts-jest",
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/lib/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/_setup.ts"
    ]
  },
  "jestSonar": {
    "reportPath": "node_modules",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "dependencies": {
    "expo-auth-session": "^5.4.0",
    "keycloak-js": "^23.0.4",
    "next-auth": "^4.24.5"
  }
}
