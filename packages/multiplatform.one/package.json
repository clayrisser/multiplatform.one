{
  "name": "multiplatform.one",
  "version": "1.0.11",
  "description": "multiplatform.one ecosystem",
  "main": "dist/cjs",
  "module": "dist/esm",
  "module:jsx": "dist/jsx",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./types/index.d.ts"
    },
    "./next": {
      "import": "./dist/esm/next/index.js",
      "require": "./dist/cjs/next/index.js",
      "types": "./types/next/index.d.ts"
    },
    "./theme": {
      "import": "./dist/esm/theme/index.js",
      "require": "./dist/cjs/theme/index.js",
      "types": "./types/theme/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rm -rf dist types 2>/dev/null && tamagui-build"
  },
  "author": "BitSpur",
  "sideEffects": true,
  "keywords": [
    "multiplatform",
    "multiplatform.one",
    "react"
  ],
  "homepage": "https://gitlab.com/risserlabs/community/auto-contrast",
  "license": "Apache-2.0",
  "files": [
    "dist",
    "next",
    "src",
    "theme",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/risserlabs/community/auto-contrast"
  },
  "peerDependencies": {
    "@storybook/react": "~7.6.19",
    "@storybook/react-native": "^6.5.3",
    "@tamagui/constants": "^1.29.3",
    "@tamagui/web": "^1.29.3",
    "@types/storybook__react-native": "^5.2.1",
    "expo-constants": "~15.4.6",
    "next": "^13.4.3",
    "react": "18.2.0",
    "react-native": "0.74.2"
  },
  "devDependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@storybook/react": "~7.6.19",
    "@storybook/react-native": "^7.6.20",
    "@tamagui/build": "^1.100.3",
    "@tamagui/constants": "^1.100.3",
    "@tamagui/web": "^1.100.3",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^15.0.7",
    "@types/react": "^18.3.1",
    "@types/react-native": "^0.73.0",
    "@types/storybook__react-native": "^5.2.1",
    "@vitejs/plugin-react": "^4.3.1",
    "expo-constants": "~16.0.2",
    "next": "^14.2.3",
    "react": "18.2.0",
    "react-native": "0.74.2",
    "ts-jest": "^29.1.4",
    "typescript": "~5.3.3",
    "vitest": "^1.6.0",
    "vitest-react-native": "^0.1.5"
  },
  "eslintIgnore": [
    "dist",
    "types"
  ],
  "dependencies": {
    "@multiplatform.one/zustand": "workspace:1.0.3",
    "cookies-next": "^4.2.1",
    "react-cookie": "^7.1.4"
  }
}