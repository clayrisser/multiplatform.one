{
  "name": "multiplatform.one",
  "version": "1.0.7",
  "description": "multiplatform.one ecosystem",
  "main": "dist/cjs",
  "module": "dist/esm",
  "module:jsx": "dist/jsx",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./types/index.d.ts"
    },
    "./next": {
      "import": "./dist/esm/next/index.js",
      "require": "./dist/cjs/next/index.js",
      "types": "./types/next/index.d.ts"
    },
    "./theme": {
      "import": "./dist/esm/theme/index.js",
      "require": "./dist/cjs/theme/index.js",
      "types": "./types/theme/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rm -rf dist types 2>/dev/null && tamagui-build"
  },
  "author": "BitSpur",
  "sideEffects": true,
  "keywords": [
    "multiplatform",
    "multiplatform.one",
    "react"
  ],
  "homepage": "https://gitlab.com/risserlabs/community/auto-contrast",
  "license": "Apache-2.0",
  "files": [
    "dist",
    "next",
    "src",
    "theme",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/risserlabs/community/auto-contrast"
  },
  "peerDependencies": {
    "@storybook/react": "^7.0.14",
    "@storybook/react-native": "^6.5.3",
    "@tamagui/constants": "^1.29.3",
    "@tamagui/web": "^1.29.3",
    "@types/storybook__react-native": "^5.2.1",
    "expo-constants": "~15.4.6",
    "next": "^13.4.3",
    "react": "18.3.1",
    "react-native": "0.74.1"
  },
  "devDependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@storybook/react": "^8.1.5",
    "@storybook/react-native": "^7.6.19",
    "@tamagui/build": "^1.99.1",
    "@tamagui/constants": "^1.99.1",
    "@tamagui/web": "^1.99.1",
    "@types/eslint": "^8.56.10",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.1",
    "@types/react-native": "^0.73.0",
    "@types/storybook__react-native": "^5.2.1",
    "cspell": "^8.8.3",
    "expo-constants": "~16.0.2",
    "jest": "^29.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "next": "^14.2.3",
    "react": "18.3.1",
    "react-native": "0.74.1",
    "ts-jest": "^29.1.4",
    "typescript": "~5.3.3"
  },
  "eslintIgnore": [
    "dist",
    "types"
  ],
  "jest": {
    "testRegex": "((/(tests|__tests__)((/|/[^_.\n][^/\n]*(/[^_.\n][^/\n]+)*/)[^_.\n][^/\n]*))|((/|/.*/)(([^_.\n][^/\n]*.(spec|test))|(spec|test)))).[jt]sx?$",
    "testEnvironment": "node",
    "verbose": true,
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/lib/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/_setup.ts"
    ]
  },
  "jestSonar": {
    "reportPath": "node_modules",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "dependencies": {
    "@multiplatform.one/zustand": "workspace:*",
    "cookies-next": "^4.2.1",
    "react-cookie": "^7.1.4"
  }
}