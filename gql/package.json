{
  "name": "gql",
  "main": "index.ts",
  "private": true,
  "sideEffects": false,
  "devDependencies": {
    "@graphql-codegen/cli": "~4.0.1",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/introspection": "4.0.0",
    "nodemon": "^3.0.2"
  },
  "nodemonConfig": {
    "watch": [
      "../api",
      "../app"
    ],
    "ext": "ts,tsx,graphql",
