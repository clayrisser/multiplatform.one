{
  "name": "multiplatform.one",
  "version": "0.5.31",
  "description": "multiplatform.one ecosystem",
  "main": "dist/lib",
  "module": "dist/esm",
  "module:jsx": "dist/jsx",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "types": "dist/lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/lib/index.js",
      "types": "./dist/lib/index.d.ts"
    },
    "./next": {
      "import": "./dist/esm/next/index.mjs",
      "require": "./dist/lib/next/index.js",
      "types": "./dist/lib/next/index.d.ts"
    },
    "./recoil": {
      "import": "./dist/esm/recoil/index.mjs",
      "require": "./dist/lib/recoil/index.js",
      "types": "./dist/lib/recoil/index.d.ts"
    },
    "./zustand": {
      "import": "./dist/esm/zustand.mjs",
      "require": "./dist/lib/zustand.js",
      "types": "./dist/lib/zustand.d.ts"
    }
  },
  "author": "Risser Labs LLC",
  "sideEffects": false,
  "keywords": [
    "multiplatform",
    "multiplatform.one",
    "react"
  ],
  "homepage": "https://gitlab.com/risserlabs/community/auto-contrast",
  "license": "Apache-2.0",
  "files": [
    "dist",
    "next",
    "recoil",
    "src",
    "zustand"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/risserlabs/community/auto-contrast"
  },
  "scripts": {
    "build": "make -s build",
    "clean": "make -s clean",
    "coverage": "make -s coverage",
    "format": "make -s format",
    "lint": "make -s lint",
    "prepare": "make -s prepare",
    "purge": "make -s purge",
    "spellcheck": "make -s spellcheck",
    "start": "make -s start",
    "test": "make -s test"
  },
  "peerDependencies": {
    "@storybook/react": "^6.5.13",
    "@storybook/react-native": "^5.3.27",
    "@tamagui/constants": "^1.1.7",
    "@types/storybook__react-native": "^5.2.1",
    "expo-constants": "^14.0.2",
    "i18next": "^22.4.9",
    "next": "^13.0.5",
    "next-i18next": "^13.0.3",
    "react": "^17.0.2",
    "react-i18next": "^12.1.4",
    "react-native": "0.68.2",
    "react-native-recoil-persist": "^0.0.6",
    "recoil-persist": "^4.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-flow": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@storybook/react": "^7.0.12",
    "@storybook/react-native": "^6.5.3",
    "@tamagui/constants": "^1.28.1",
    "@types/eslint": "^8.37.0",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.2.6",
    "@types/react-native": "^0.72.0",
    "@types/storybook__react-native": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "babel-plugin-fully-specified": "^1.3.0",
    "browserslist": "^4.21.5",
    "cspell": "^6.31.1",
    "esbuild": "^0.17.19",
    "esbuild-plugin-babel": "^0.2.3",
    "eslint": "^8.40.0",
    "eslint-config-alloy": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-spellcheck": "^0.0.20",
    "expo-constants": "^14.2.1",
    "i18next": "^22.5.0",
    "jest": "^29.5.0",
    "jest-sonar-reporter": "^2.0.0",
    "lockfile-lint": "^4.10.1",
    "next": "^13.4.3",
    "next-i18next": "^13.2.2",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-i18next": "^12.3.1",
    "react-native": "0.71.8",
    "react-native-recoil-persist": "^0.0.6",
    "recoil-persist": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "resolutions": {
    "@types/react": "^18.2.6"
  },
  "packageManager": "yarn@3.3.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "eslintIgnore": [
    "dist"
  ],
  "jest": {
    "testRegex": "((/(tests|__tests__)((/|/[^_.\n][^/\n]*(/[^_.\n][^/\n]+)*/)[^_.\n][^/\n]*))|((/|/.*/)(([^_.\n][^/\n]*.(spec|test))|(spec|test)))).[jt]sx?$",
    "testEnvironment": "node",
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/_setup.ts"
    ]
  },
  "jestSonar": {
    "reportPath": ".mkpm/.tmp/reports",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "prettier": "eslint-config-alloy/.prettierrc.js",
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.18.1",
    "zustand": "^4.3.8",
    "zustand-tools": "^0.0.7"
  }
}
