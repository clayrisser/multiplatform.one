{
  "name": "prisma-scripts",
  "version": "0.0.2",
  "description": "scripts for managing orm",
  "keywords": [
    "prisma-scripts",
    "prisma",
    "scripts",
    "orm",
    "database",
    "db"
  ],
  "homepage": "https://github.com/codejamninja/prisma-scripts",
  "bugs": {
    "url": "https://github.com/codejamninja/prisma-scripts/issues",
    "email": "jam@codejam.ninja"
  },
  "license": "MIT",
  "author": {
    "name": "Jam Risser",
    "email": "jam@codejam.ninja",
    "url": "https://codejam.ninja"
  },
  "files": [
    "lib",
    "scripts"
  ],
  "bin": {
    "generate": "lib/generate.js",
    "wait-for-postgres": "lib/waitForPostgres.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codejamninja/prisma-scripts"
  },
  "scripts": {
    "build": "make -s build",
    "clean": "make -s clean",
    "coverage": "make -s coverage",
    "format": "make -s format",
    "inc": "npm version patch --git=false 2>/dev/null || true",
    "lint": "make -s lint",
    "make": "make",
    "prepare": "make -s prepare",
    "purge": "make -s purge",
    "report": "make -s report",
    "spellcheck": "make -s spellcheck",
    "start": "make -s start",
    "test": "make -s test",
    "test:ui": "make test-ui",
    "test:watch": "make -s test-watch"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@prisma/client": "^2.6.2",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "execa": "^4.0.3",
    "fs-extra": "^9.0.1",
    "ora": "^5.1.0",
    "pkg-dir": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.13",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "cspell": "^4.1.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.4.2",
    "jest-sonar-reporter": "^2.0.0",
    "lockfile-lint": "^4.3.7",
    "majestic": "^1.7.0",
    "makefiles": "^0.1.1",
    "prettier": "^2.1.1",
    "typescript": "^4.0.2"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "eslintIgnore": [
    "lib"
  ],
  "jest": {
    "testRegex": "tests(/|/.*/)[^_/]*[jt]sx?$",
    "verbose": true
  },
  "jestSonar": {
    "reportPath": "node_modules/.tmp/reports",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
